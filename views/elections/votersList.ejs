<!DOCTYPE html>
<html>
  <head>
    <% include ../partials/head %>
  </head>
  <body>
    <% include ../partials/adminHead %>
    <div class="container bodyContainer">
          <div class="row">
          <div class="col-4">
            <a href="/elections/voters/createVoter"><button class="btn btn-success">Create Voter</button></a>
          </div>
        </div>

              <div class="touchdown">

                    <table class="table">
                          <tr>
                              <td>Name</td>
                              <td>District No.</td>
                              <td>Address</td>
                          </tr>
                            <% for(var i=0; i< voters.length; i++) {%>
                                <tr>
                                  <td><%= voters[i].firstName +' '+ voters[i].lastName %></td>
                                  <td><%= voters[i].district %></td>
                                  <td><%= voters[i].address %></td>
                                  <td><a class="btn btn-primary" href="/elections/voters/edit/<%= voters[i].id%>" >Edit</a></td>
                                  <td><button class="btn btn-danger deleteVoter" value="<%= voters[i].id%>">Delete</button></td>
                                </tr>

                          <% }  %>
                      </table>


              </div>

        </div>
  <% include ../partials/footer %>
  </body>

  </html>
